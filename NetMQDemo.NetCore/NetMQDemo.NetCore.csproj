<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NetMQ" Version="4.0.0.207" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="CSForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="CSForm.Designer.cs">
      <DependentUpon>CSForm.cs</DependentUpon>
    </Compile>
    <Compile Update="PubSubForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="PubSubForm.Designer.cs">
      <DependentUpon>PubSubForm.cs</DependentUpon>
    </Compile>
    <Compile Update="PushPullForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="PushPullForm.Designer.cs">
      <DependentUpon>PushPullForm.cs</DependentUpon>
    </Compile>
  </ItemGroup>

</Project>